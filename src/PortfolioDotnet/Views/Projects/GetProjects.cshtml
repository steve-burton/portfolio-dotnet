@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Portfolio - About";
}

@using PortfolioDotnet.Models
@model List<Projects>

<div class="intro">
    <h3>Here are my three most-starred projects</h3>
    <div>
        @foreach (var project in Model)
        {
            <p>@Html.DisplayFor(modelItem => project.Name)</p>
        }
    </div>
</div>